{
  "expo": {
    "name": "SantiTime",
    "slug": "santitime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/logo-bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "userInterfaceStyle": "dark"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/logo-bg.png"
      },
      "package": "com.a.kolodynsky.santitime",
      "userInterfaceStyle": "dark"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./src/shared/assets/images/splash.png",
          "backgroundColor": "#1a1a24"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3dc4b6a0-d08f-4783-b6b1-12c89249b607"
      }
    },
    "jsEngine": "hermes"
  }
}
